<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" fx:id="meilleursScoresController" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controler.MeilleursScoresController" style="-fx-background-color: #cce6ff;">
    <Button text="Retour" layoutX="10.0" layoutY="10.0" onAction="#handleRetourButton"/>
    <TableView fx:id="scoresTable" prefHeight="200.0" prefWidth="400.0" layoutX="100.0" layoutY="100.0">
        <columns>
            <TableColumn text="Nom d'utilisateur" fx:id="usernameColumn"/>
            <TableColumn text="Score" fx:id="scoreColumn"/>
        </columns>
    </TableView>
</AnchorPane>
